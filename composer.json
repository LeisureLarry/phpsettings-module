    "require": {
        "php": ">=5.5.0",
        "zendframework/zend-modulemanager": "*",
        "zendframework/zend-mvc": "*"
    },
    "require-dev": {
        "polderknowledge/build-tools": "dev-master@dev"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://developers.polderknowledge.nl/packages"
        }
    ],
    "autoload": {
